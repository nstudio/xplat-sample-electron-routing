{
  "name": "companytesting",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall.web && ng serve web-myapp",
    "build": "npm run postinstall.web && ng build web-myapp",
    "test": "npm run postinstall.web && ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "postinstall": "electron-rebuild install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron.myapp": "npm run prepare.electron.myapp && ng build electron-myapp --prod --base-href ./",
    "build.electron.myapp.local": "npm run build.electron.myapp && electron dist/apps/electron-myapp",
    "build.electron.myapp.linux": "npm run build.electron.myapp && cd dist/apps/electron-myapp && npx electron-builder build --linux",
    "build.electron.myapp.windows": "npm run build.electron.myapp && cd dist/apps/electron-myapp && npx electron-builder build --windows",
    "build.electron.myapp.mac": "npm run build.electron.myapp && cd dist/apps/electron-myapp && npx electron-builder build --mac",
    "prepare.electron.myapp": "npm run postinstall.electron && tsc -p apps/electron-myapp/tsconfig.json",
    "serve.electron.myapp.target": "ng serve electron-myapp",
    "serve.electron.myapp": "wait-on http-get://localhost:4200/ && electron apps/electron-myapp/src --serve",
    "start.electron.myapp": "npm run prepare.electron.myapp && npm-run-all -p serve.electron.myapp.target serve.electron.myapp",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "start.web.myapp": "npm run postinstall.web && ng serve web-myapp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.4.0",
    "zone.js": "^0.8.26",
    "@nrwl/nx": "~7.6.2",
    "@ngrx/effects": "~7.3.0",
    "@ngrx/router-store": "~7.3.0",
    "@ngrx/store": "~7.3.0",
    "@ngx-translate/core": "~11.0.0",
    "@ngx-translate/http-loader": "~4.0.0",
    "@mycompany/scss": "file:libs/scss",
    "reflect-metadata": "^0.1.12",
    "@mycompany/web": "file:xplat/web",
    "@angular/http": "~7.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/schematics": "7.6.2",
    "@nstudio/schematics": "^7.3.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.4",
    "electron-installer-dmg": "~2.0.0",
    "electron-packager": "~13.1.0",
    "electron-reload": "~1.4.0",
    "electron-store": "~2.0.0",
    "electron-updater": "~4.0.6",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "0.2.4",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "prettier": "1.13.7",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "wait-on": "~3.2.0",
    "electron-rebuild": "~1.8.4",
    "terser-webpack-plugin": "~1.2.0"
  },
  "xplat": {
    "prefix": "abc"
  }
}
